DECLARE PLUGIN "coq-completion.plugin"

{

open! Pp
open! Ltac_plugin

}

VERNAC COMMAND EXTEND HelloWorld CLASSIFIED AS QUERY
| [ "HelloWorld" ] -> { Feedback.msg_notice (strbrk Msg.message) }
END

TACTIC EXTEND hello_world_tactic
| [ "hello_world" ] ->
  { let _ = Feedback.msg_notice (str Msg.message) in
    Tacticals.tclIDTAC }
END
